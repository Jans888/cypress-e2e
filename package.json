{
  "name": "cypress-e2e",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:run": "cypress run --spec \"cypress/integration/regression/*\"",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:clear": "if exist allure-results rmdir /q /s allure-results && if exist allure-report rmdir /q /s allure-report && if exist cypress\\screenshots rmdir /q /s cypress\\screenshots && if exist cypress\\videos rmdir /q /s cypress\\videos",
    "allure:pretest": "npm run allure:clear",
    "allure:test": "npm run cy:run || npm run allure:posttest",
    "allure:posttest": "npm run allure:report",
    "mocha:clear": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochareports",
    "mocha:pretest": "npm run mocha:clear",
    "scripts": "cypress run",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "mocha:posttest": "npm run combine-reports && npm run generate-report",
    "mocha:test": "npm run scripts || npm run allure:posttest",
    "cy:runApi": "cypress run --reporter mochawesome"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.14.0",
    "cypress": "^8.4.1",
    "cypress-file-upload": "^5.0.8",
    "cypress-log-to-output": "^1.1.2",
    "cypress-mochawesome-reporter": "^2.3.0",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-real-events": "^1.5.1",
    "dayjs": "^1.10.7",
    "faker": "^5.5.3",
    "gmail-tester": "^1.3.3",
    "jsonpath": "^1.1.1",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "pdf-parse": "^1.1.1",
    "typescript": "^4.4.3",
    "webpack": "^5.53.0"
  }
}
